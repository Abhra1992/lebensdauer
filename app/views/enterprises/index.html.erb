<h1>Listing enterprises</h1>

<table>
  <tr>
    <th>Registration</th>
    <th>Name</th>
    <th>Category</th>
    <th>Email</th>
    <th>Fax</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @enterprises.each do |enterprise| %>
  <tr>
    <td><%= enterprise.registration %></td>
    <td><%= enterprise.name %></td>
    <td><%= enterprise.category %></td>
    <td><%= enterprise.email %></td>
    <td><%= enterprise.fax %></td>
    <td><%= link_to 'Show', enterprise %></td>
    <td><%= link_to 'Edit', edit_enterprise_path(enterprise) %></td>
    <td><%= link_to 'Destroy', enterprise, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

